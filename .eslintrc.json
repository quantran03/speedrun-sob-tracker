{
    "env": {
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "extends": [
      "eslint:recommended"
     ],
    "rules": {
        "quotes": [2, "double", { "avoidEscape": true }],
        "indent": ["error", 4]
    }
}